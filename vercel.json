{
  "buildCommand": "cd opencpai-app && npm install && npm run build",
  "outputDirectory": "opencpai-app/build",
  "installCommand": "cd opencpai-app && npm install",
  "framework": null,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/:path*" }
  ]
}
